x=5
y=8
print(" x == y :", x == y)
print(" x != y :", x != y)
print(" x < y :", x < y)
print(" x > y :", x > y)
print(" x <= y :", x <= y)
print(" x >= y :", x >= y)
Welcome = "Welcome"
welcome = "welcome"
print("Welcome==welcome:",Welcome==welcome)
